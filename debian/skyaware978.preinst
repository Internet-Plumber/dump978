#!/bin/sh

set -e

# preinst rm_conffile checks if the conffile has been modified, and renames it either to conffile.dpkg-remove (if not modified) or to conffile.dpkg-backup (if modified)
if dpkg-maintscript-helper supports rm_conffile 2>/dev/null
then
	dpkg-maintscript-helper rm_conffile /etc/lighttpd/conf-enabled/89-skyview978.conf 3.7.1 -- "$@"
fi
